{
  "name": "AutoUnfollow",
  "description": "Auto-unchecks 'Follow company' during LinkedIn Easy Apply",
  "version": "0.0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/jobs/search/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ]
}
