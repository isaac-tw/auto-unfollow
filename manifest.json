{
  "name": "AutoUnfollow",
  "description": "Auto-unchecks 'Follow company' during LinkedIn Easy Apply",
  "version": "1.0.1",
  "manifest_version": 3,
  "content_scripts": [
    {
      "matches": ["https://www.linkedin.com/jobs/*"],
      "js": ["content.js"],
      "run_at": "document_idle"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
